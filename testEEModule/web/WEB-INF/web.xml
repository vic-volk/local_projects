<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
           version="2.5">

    <servlet>
        <servlet-name>EEModule</servlet-name>
        <servlet-class>ru.bmstu.test.eemodule.base.EntryPoint</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>EEModule</servlet-name>
        <url-pattern>/EEModule</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AuthorizationServlet</servlet-name>
        <servlet-class>ru.bmstu.test.eemodule.base.AuthorizationServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AuthorizationServlet</servlet-name>
        <url-pattern>/AuthorizationServlet</url-pattern>
    </servlet-mapping>

</web-app>
